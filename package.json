{
  "name": "webinar-geek-test",
  "version": "1.0.0",
  "description": "Electron-based desktop application for real-time Webinar Geek attendance logging and export",
  "author": "Finn Stolle",
  "main": "main.js",
  "build": {
    "appId": "com.deinfirma.webinarrecorder",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --mac --win"
  },
  "dependencies": {
    "@sparticuz/chromium": "^133.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.20.0",
    "puppeteer": "^24.7.1",
    "puppeteer-core": "^24.7.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "electron-builder": "^26.0.12",
    "electron": "^36.2.0"
  }
}
